export type Bitfield = string;

export type Color = string;

export type ColorContainer<T> = [T, T];

export interface BoardData {
	player_color: Color;
	bitfield: ColorContainer<Bitfield>;
}

export type Board = BoardData;

export type ByteSize = number;

export type Depth = number;

export type ForbiddenKind = string;

export type Pos = PosSchema;

export type MaybePos = Pos | undefined;

export type History = MaybePos[];

export interface GameStateData {
	board: Board;
	history: History;
}

export type GameState = GameStateData;

export type HashKey = string;

export type PrincipalVariation = MaybePos[];

export type RuleKind = string;

export type Score = number;

export type SearchObjective = string;

export type SessionStatus = string;

export interface DurationSchema {
	secs: number;
	nanos: number;
}

export interface BestMove {
	position_hash: HashKey;
	best_move: MaybePos;
	score: Score;
	selective_depth: number;
	total_nodes_in_1k: number;
	pv: PrincipalVariation;
	time_elapsed: DurationSchema;
}

export interface ComputingResource {
	workers: number;
	tt_size: ByteSize;
	time: DurationSchema;
	nodes_in_1k?: number;
}

export interface Timer {
	total_remaining: DurationSchema;
	increment: DurationSchema;
	turn: DurationSchema;
}

export interface Config {
	rule_kind: RuleKind;
	draw_condition: number;
	max_nodes_in_1k: number;
	max_depth: Depth;
	max_vcf_depth: Depth;
	tt_size: ByteSize;
	workers: number;
	pondering: boolean;
	dynamic_time: boolean;
	initial_timer: Timer;
	spawn_depth_specialist: boolean;
}


{
  "name": "mintaka-webui",
  "version": "0.0.1",
  "description": "",
  "type": "module",
  "scripts": {
    "wasm:build": "cargo wasm-build --manifest-path ../Cargo.toml --release -p mintaka_wasm",
    "wasm:bindgen": "wasm-bindgen ../target/wasm32-unknown-unknown/release/mintaka_wasm.wasm --out-dir src/wasm/pkg --target web",
    "wasm:init": "pnpm wasm:build && pnpm wasm:bindgen",

    "dev:wasm": "pnpm wasm:init && vite",
    "dev": "vite",
    "build": "pnpm wasm:init && vite build",
    "serve": "pnpm wasm:init && vite preview"
  },
  "devDependencies": {
    "typescript": "^5.9.2",
    "solid-devtools": "^0.34.3",

    "vite": "^7.1.11",
    "vite-plugin-solid": "^2.11.8",

    "tailwindcss": "^4.1.13",
    "@tailwindcss/vite": "^4.1.13",
    "daisyui": "^5.5.11"
  },
  "dependencies": {
    "solid-js": "^1.9.0"
  }
}

{
  "name": "mintaka-webui",
  "version": "0.0.1",
  "description": "",
  "type": "module",
  "scripts": {
    "format": "eslint src --fix",
    "requirements": "rustup component add rust-src && rustup target add wasm32-unknown-unknown && cargo install wasm-pack typeshare-cli",
    "typeshare:build": "typeshare ../ --lang=typescript --output-file=../mintaka_wasm/types_ts",
    "wasm:build": "wasm-pack build ../mintaka_wasm --release --target web --out-name mintaka_wasm --out-dir ../mintaka_webui/src/wasm/pkg/mintaka_wasm -- -Z build-std=std,panic_abort",
    "wasm-worker:build": "wasm-pack build ../mintaka_wasm_worker --release --target web --out-name mintaka_wasm_worker --out-dir ../mintaka_webui/src/wasm/pkg/mintaka_wasm_worker -- -Z build-std=std,panic_abort",
    "wasm:package": "pnpm run typeshare:build && pnpm run wasm:build && pnpm run wasm-worker:build",
    "typecheck": "tsc --noEmit --strict",
    "dev:wasm": "pnpm run wasm:package && vite --host",
    "dev": "vite --host",
    "build": "pnpm run wasm:package && vite build",
    "serve": "pnpm run build && vite preview --host"
  },
  "devDependencies": {
    "@stylistic/eslint-plugin": "^5.6.1",
    "@tailwindcss/vite": "^4.1.18",
    "@typescript-eslint/parser": "^8.51.0",
    "daisyui": "^5.5.14",
    "eslint": "^9.39.2",
    "eslint-plugin-better-tailwindcss": "^3.8.0",
    "solid-devtools": "^0.34.4",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vite-plugin-solid": "^2.11.10"
  },
  "dependencies": {
    "@solid-primitives/storage": "^4.3.3",
    "solid-js": "^1.9.10"
  }
}
